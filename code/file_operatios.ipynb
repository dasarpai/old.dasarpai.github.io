    "def change_text_in_file(filename):\n",
    "    f = open(filename, \"rt\", encoding=\"utf8\")\n",
    "    # read all lines\n",
    "    all_lines = f.readlines(0)\n",
    "    f.close()\n",
    "\n",
    "    line_no=0\n",
    "    for line in all_lines:\n",
    "        #fine location of word\n",
    "        search_word=\"[~English](/pmbok6/\"\n",
    "        search_word_len = len(search_word)\n",
    "        location = line.find(search_word)\n",
    "        if location>-1:\n",
    "            found_word = line[search_word_len + location :]\n",
    "            converted_word = found_word.title()\n",
    "\n",
    "            converted_line = line[0:search_word_len + location]+converted_word\n",
    "\n",
    "            all_lines[line_no]=converted_line\n",
    "        line_no=line_no+1\n",
    "\n",
    "    new_text = \"\".join(all_lines)\n",
    "\n",
    "\n",
    "    f = open(filename, \"w\", encoding=\"utf8\")\n",
    "    f.write(new_text)\n",
    "    f.close()\n",
    "\n",
    "    "
