    "import os \n",
    "folder = 'D:\\github-blog\\wiapost-old'\n",
    "for fname in os.listdir(folder):\n",
    "    fname=fname.title()[:-3]+\".md\"\n",
    "    f = open(folder+\"\\\\\"+ fname, \"rt\", encoding=\"utf8\")\n",
    "    # read all lines\n",
    "    all_lines = f.readlines(0)\n",
    "    i=0\n",
    "    for line in all_lines:\n",
    "        if line.lstrip()[0:2]==\"id\":\n",
    "            id = line.lstrip().rstrip()[-4:]\n",
    "        if line.lstrip()[0:9] == \"permalink\":\n",
    "            slug= line.split(\"/\")[-1].replace(\"\\n\",\"\")\n",
    "        if line.lstrip()[0:6]==\"teaser\":\n",
    "            img = '/assets/images/wiapost/'+id+\"-\"+slug.title()+\".jpg\" \n",
    "            all_lines[i] = \"     teaser: \"+img + \"\\n\"\n",
    "        if line.lstrip()[0:2] == '![':\n",
    "            all_lines[i]=\"![\"+slug.title().replace(\"-\",\" \")+\"](\"+img+\")\"+\"\\n\"\n",
    "        i=i+1\n",
    "    f.close()\n",
    "    text = \"\".join(all_lines)\n",
    "\n",
    "    f = open(\"md-files\\\" +fname, \"w\",encoding=\"utf8\")\n",
    "    f.write(text)\n",
    "    f.close()\n",
    "\n",
    "    print(fname)\n"
   ]
